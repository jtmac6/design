package Design;

import java.util.List;

/** 
 * @author artur
 */
public interface WindowFactory {

	/**
	 * @return  Returns the _imgList.
	 * @uml.property  name="_imgList"
	 */
	public List get_imgList();

	/**
	 * Setter of the property <tt>_imgList</tt>
	 * @param _imgList  The _imgList to set.
	 * @uml.property  name="_imgList"
	 */
	public void set_imgList(List _imgList);

		
		/**
		 */
		public abstract void CreateWindow(List imgList);
		

}
